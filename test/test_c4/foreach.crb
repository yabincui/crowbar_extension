function f1() {
	println("before");
	for (i = 0; i < 3; i++) {
		println("i = " + i);
		continue;
		break;
	}
	println("after");

	i = 0;
	println("i = " + i);
	while (i < 3) {
		println("i = " + i);
		i++;
		
	}
}
f1();
print("after f1()\n");


